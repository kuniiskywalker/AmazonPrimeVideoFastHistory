{
    "name": "AmazonVideoFastHistory",
    "manifest_version": 2,
    "version": "1.1",
    "default_locale": "en",
    "description": "This extention adds viewing history links to Amazon videos.",
    "icons" : {
        "32" : "icon32.png",
        "48" : "icon48.png",
        "128" : "icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://www.amazon.co.jp/*"],
        "js": ["build/bundle.js"],
        "run_at": "document_end"
    }]
}
