{
    "name": "__MSG_appName__",
    "manifest_version": 2,
    "version": "1.5.4",
    "default_locale": "en",
    "description": "__MSG_appDesc__",
    "icons" : {
        "32" : "icon32.png",
        "48" : "icon48.png",
        "128" : "icon128.png"
